  <code>// if this goes anywhere throw this behind a loading screen

show_debug_overlay(true);
application_surface_draw_enable(false);

var s=32;

global.model_floor=d3d_model_create();

d3d_model_primitive_begin(global.model_floor, pr_trianglelist);

for (var i=0; i&lt;=BATTLEFIELD_SIZE; i=i+s){
    for (var j=0; j&lt;=BATTLEFIELD_SIZE; j=j+s){
        d3d_model_vertex_normal_texture(global.model_floor, i, j, 0, 0, 0, 1, 0, 0);
        d3d_model_vertex_normal_texture(global.model_floor, i+32, j, 0, 0, 0, 1, 1, 0);
        d3d_model_vertex_normal_texture(global.model_floor, i+32, j+32, 0, 0, 0, 1, 1, 1);
        
        d3d_model_vertex_normal_texture(global.model_floor, i+32, j+32, 0, 0, 0, 1, 1, 1);
        d3d_model_vertex_normal_texture(global.model_floor, i, j+32, 0, 0, 0, 1, 0, 1);
        d3d_model_vertex_normal_texture(global.model_floor, i, j, 0, 0, 0, 1, 0, 0);
    }
}

d3d_model_primitive_end(global.model_floor);

// end setup

room_goto(main);</code>
